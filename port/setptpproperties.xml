<edit-config>
    <target>
      <running/>
    </target>
    <config>
      <waveserver-ptps xmlns="urn:ciena:params:xml:ns:yang:ciena-ws:ciena-waveserver-ptp"
                       xmlns:ncx="http://netconfcentral.org/ns/yuma-ncx">
        <ptps>
          <ptp-id>2-1</ptp-id>
          <properties>
            <xcvr-type>WaveLogic Ai</xcvr-type>
            <forward-error-correction>enabled</forward-error-correction>
            <thresholds>
              <pre-fec-sf-dbq>0.0</pre-fec-sf-dbq>
              <pre-fec-sd-dbq>0.50</pre-fec-sd-dbq>
            </thresholds>
            <transmitter>
              <state>enabled</state>
              <frequency xmlns="urn:ciena:params:xml:ns:yang:ciena-ws:ciena-waveserver-ptp-modem">
                <value>195562.5</value>
              </frequency>
              <power xmlns="urn:ciena:params:xml:ns:yang:ciena-ws:ciena-waveserver-ptp-modem">
                <value>1.0</value>
              </power>
            </transmitter>
            <line-system xmlns="urn:ciena:params:xml:ns:yang:ciena-ws:ciena-waveserver-ptp-modem">
              <type>coloured</type>
            </line-system>
            <modem xmlns="urn:ciena:params:xml:ns:yang:ciena-ws:ciena-waveserver-ptp-modem">
              <channel-contention-detection-avoidance>disabled</channel-contention-detection-avoidance>
              <class>terrestrial</class>
              <expected-restoration-time>10</expected-restoration-time>
              <laser-centering-mode>enabled</laser-centering-mode>
              <laser-centering-range>2.5</laser-centering-range>
              <laser-shutdown-mode>accelerated</laser-shutdown-mode>
              <laser-tuning-mode>normal</laser-tuning-mode>
              <link-optimization-mode>standard</link-optimization-mode>
              <reach-limit>1000</reach-limit>
              <tx-dispersion-compensation-mode>automatic</tx-dispersion-compensation-mode>
              <tx-dispersion-precompensation-value>0</tx-dispersion-precompensation-value>
              <tx-path-dispersion-value>0</tx-path-dispersion-value>
              <tx-power-reduction-state>disabled</tx-power-reduction-state>
            </modem>
          </properties>
        </ptps>
        <ptps>
          <ptp-id>2-3</ptp-id>
          <properties>
            <xcvr-type>QSFP28</xcvr-type>
            <forward-error-correction>disabled</forward-error-correction>
            <transmitter>
              <state>enabled</state>
            </transmitter>
          </properties>
        </ptps>
        <ptps>
          <ptp-id>2-4</ptp-id>
          <properties>
            <xcvr-type>QSFP28</xcvr-type>
            <forward-error-correction>disabled</forward-error-correction>
            <transmitter>
              <state>enabled</state>
            </transmitter>
          </properties>
        </ptps>
      </waveserver-ptps>
    </config>
</edit-config>