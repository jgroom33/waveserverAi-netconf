<edit-config>
    <target>
      <running/>
    </target>
    <config>
      <waveserver-ports xmlns="urn:ciena:params:xml:ns:yang:ciena-ws:ciena-waveserver-port"
                        xmlns:ncx="http://netconfcentral.org/ns/yuma-ncx">
        <ports>
          <port-id>2-1</port-id>
          <channels>
            <channel-id>1</channel-id>
            <properties>
              <odu-sd-threshold>1E-9</odu-sd-threshold>
              <trace>
                <mismatch-mode>operator-only</mismatch-mode>
                <mismatch-fail-mode>none</mismatch-fail-mode>
                <tx-sapi/>
                <tx-dapi/>
                <tx-oper/>
                <tx-oper-mode>automatic</tx-oper-mode>
                <exp-sapi/>
                <exp-dapi/>
                <exp-oper/>
              </trace>
              <encryption xmlns="urn:ciena:params:xml:ns:yang:ciena-ws:ciena-waveserver-encryption">
                <pre-shared-key>
                  <description>hex16.psk</description>
                  <value>*</value>
                </pre-shared-key>
              </encryption>
            </properties>
          </channels>
          <channels>
            <channel-id>2</channel-id>
            <properties>
